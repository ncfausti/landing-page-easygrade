  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/cz-commitlint": "^17.4.4",
    "@next/eslint-plugin-next": "^14.2.3",
    "@playwright/test": "^1.31.0",
    "@semantic-release/changelog": "^6.0.2",
